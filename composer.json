{
    "name": "webbj74/toggl-php",
    "type": "library",
    "description": "An experimental PHP SDK for Toggl.",
    "keywords": ["toggl"],
    "homepage": "https://github.com/webbj74/toggl-php",
    "license": "MIT",
    "authors": [
        {
            "name": "See contributors",
            "homepage": "https://github.com/webbj74/toggl-php/graphs/contributors"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "guzzlehttp/uri-template": "*"
    },
    "require-dev": {
        "phpmd/phpmd": "~1.0",
        "phpunit/phpunit": "^9.0",
        "php-coveralls/php-coveralls": "~2.0",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-0": {
            "Toggl": "src/"
        }
    }
}
